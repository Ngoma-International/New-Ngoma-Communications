{
    "name": "scott/file-pont",
    "description": "Helps to upload images in drag and drop system",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "scott Tresor",
            "email": "scotttresor@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Scott\\FilePont\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scott\\FilePont\\Tests": "tests/"
        }
    },
    "extra": {
