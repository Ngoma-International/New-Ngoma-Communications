            "providers": [
                "Scott\\FilePont\\FilePontServiceProvider"
            ],
            "aliases": {
                "FilePont": "Scott\\FilePont\\Facades\\FilePont"
            }
        }
    },
    "require": {
        "php": "^8.1"
    }
}
