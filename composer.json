{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5.0",
        "itsgoingd/clockwork": "^5.1.8",
        "jantinnerezo/livewire-alert": "^2.2",
        "laravel/framework": "^9.35.1",
        "laravel/sanctum": "^2.15.1",
        "laravel/tinker": "^2.7.2",
        "laravel/ui": "^4.0",
        "livewire/livewire": "^2.10",
        "owen-oj/laravel-getid3": "^2.1",
        "realrashid/sweet-alert": "^5.1",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-view-models": "^1.5",
        "symfony/filesystem": "^6.1",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.20.0",
        "laravel-fans/lint": "^1.0",
        "laravel/sail": "^1.16.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "nunomaduro/larastan": "^2.2",
        "nunomaduro/phpinsights": "^2.6",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^9.5.25",
        "spatie/laravel-ignition": "^1.5.2",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.29"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "static-analyse": [
            "parallel-lint app tests",
            "phpcs --standard=PSR12 app tests",
            "phpmd app,tests ansi phpmd.xml",
            "phpstan analyse"
        ]

    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
